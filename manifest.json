{
  "name": "Standby lb detector",
  "version": "1.0",
  "author": "Magnus Watn",
  "manifest_version": 2,
  "icons": {
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "permissions": ["https://*/", "webRequest"],
  "page_action": {
    "default_icon": {
      "64": "icon64.png"
    }
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "js": ["contentScript.js"],
      "css": ["standby-lb-detector-css.css"],
      "matches": ["https://*/xui/"]
    }
  ]
}
